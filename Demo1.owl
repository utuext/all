###  http://www.semanticweb.org/mruge/ontologies/2021/11/pizaa#borders
:borders rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf owl:topObjectProperty ;
         rdf:type owl:SymmetricProperty ;
         rdfs:domain :Country ;
         rdfs:range :Country .


###  http://www.semanticweb.org/mruge/ontologies/2021/11/pizaa#containsState
:containsState rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf owl:topObjectProperty ;
               owl:inverseOf :isPartOf .


###  http://www.semanticweb.org/mruge/ontologies/2021/11/pizaa#isCapitalOf
:isCapitalOf rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :islocatedIn ;
             rdf:type owl:FunctionalProperty .


###  http://www.semanticweb.org/mruge/ontologies/2021/11/pizaa#isPartOf
:isPartOf rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf owl:topObjectProperty ;
          rdf:type owl:FunctionalProperty ;
          rdfs:domain :State ;
          rdfs:range :Country .


###  http://www.semanticweb.org/mruge/ontologies/2021/11/pizaa#islocatedIn
:islocatedIn rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf owl:topObjectProperty ;
             rdf:type owl:FunctionalProperty ;
             rdfs:domain :City ;
             rdfs:range :Country .


###  http://www.semanticweb.org/mruge/ontologies/2021/11/pizaa#nearbyCountry
:nearbyCountry rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf owl:topObjectProperty ;
               owl:propertyChainAxiom ( :islocatedIn
                                        :borders
                                      ) .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/mruge/ontologies/2021/11/pizaa#population
:population rdf:type owl:DatatypeProperty ;
            rdfs:subPropertyOf owl:topDataProperty ;
            rdf:type owl:FunctionalProperty ;
            rdfs:domain :Place ;
            rdfs:range xsd:integer .


#################################################################
#    Classes
#################################################################

###  http://www.semanticweb.org/mruge/ontologies/2021/11/pizaa#CentralEuropenCountry
:CentralEuropenCountry rdf:type owl:Class ;
                       rdfs:subClassOf :EuropenCountry ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty :borders ;
                                         owl:allValuesFrom :EuropenCountry
                                       ] .


###  http://www.semanticweb.org/mruge/ontologies/2021/11/pizaa#City
:City rdf:type owl:Class ;
      rdfs:subClassOf :Place .


###  http://www.semanticweb.org/mruge/ontologies/2021/11/pizaa#Country
:Country rdf:type owl:Class ;
         rdfs:subClassOf :Place .


###  http://www.semanticweb.org/mruge/ontologies/2021/11/pizaa#EuropenCountry
:EuropenCountry rdf:type owl:Class ;
                rdfs:subClassOf :Country .


###  http://www.semanticweb.org/mruge/ontologies/2021/11/pizaa#EuropenNeighborCountry
:EuropenNeighborCountry rdf:type owl:Class ;
                        rdfs:subClassOf :NonEuropernCountry ,
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty :borders ;
                                          owl:someValuesFrom :EuropenCountry
                                        ] .


###  http://www.semanticweb.org/mruge/ontologies/2021/11/pizaa#GermanCity
:GermanCity rdf:type owl:Class ;
            rdfs:subClassOf :City ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :islocatedIn ;
                              owl:hasValue :Germany
                            ] .


###  http://www.semanticweb.org/mruge/ontologies/2021/11/pizaa#IslandCountry
:IslandCountry rdf:type owl:Class ;
               rdfs:subClassOf :Country ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :borders ;
                                 owl:qualifiedCardinality "0"^^xsd:nonNegativeInteger ;
                                 owl:onClass :Country
                               ] .


###  http://www.semanticweb.org/mruge/ontologies/2021/11/pizaa#NonEuropernCountry
:NonEuropernCountry rdf:type owl:Class ;
                    owl:equivalentClass [ rdf:type owl:Class ;
                                          owl:complementOf :EuropenCountry
                                        ] ;
                    rdfs:subClassOf :Country .


###  http://www.semanticweb.org/mruge/ontologies/2021/11/pizaa#NorthenCountry
:NorthenCountry rdf:type owl:Class ;
                rdfs:subClassOf :Country .


###  http://www.semanticweb.org/mruge/ontologies/2021/11/pizaa#NorthenEuropenCountry
:NorthenEuropenCountry rdf:type owl:Class ;
                       owl:equivalentClass [ owl:intersectionOf ( :EuropenCountry
                                                                  :NorthenCountry
                                                                ) ;
                                             rdf:type owl:Class
                                           ] .


###  http://www.semanticweb.org/mruge/ontologies/2021/11/pizaa#Place
:Place rdf:type owl:Class .


###  http://www.semanticweb.org/mruge/ontologies/2021/11/pizaa#Region
:Region rdf:type owl:Class ;
        owl:equivalentClass [ rdf:type owl:Class ;
                              owl:unionOf ( :Country
                                            :State
                                          )
                            ] .


###  http://www.semanticweb.org/mruge/ontologies/2021/11/pizaa#State
:State rdf:type owl:Class ;
       rdfs:subClassOf :Place .


